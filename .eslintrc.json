{
	"env": {
		"browser": true,
		"es2022": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"plugin:vue/vue3-recommended",
		"prettier"
	],
	"ignorePatterns": ["dist/", "node_modules/"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"parser": "@typescript-eslint/parser",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint", "vue"],
	"rules": {
		"vue/multi-word-component-names": ["off"]
	}
}
